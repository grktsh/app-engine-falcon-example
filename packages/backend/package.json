{
  "scripts": {
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.py": [
      "pipenv run isort --atomic",
      "pipenv run yapf --in-place --exclude '*/tests/appengine.py'",
      "pipenv run flake8",
      "git add"
    ]
  },
  "devDependencies": {
    "lint-staged": "^7.2.0"
  }
}
